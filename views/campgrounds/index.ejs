<% layout('layouts/boilerplate')%>
    <a href="/campgrounds/new">Make A New Campground</a>
    <h1>All Campgrounds</h1>
    <ul>
        <% for(let c of campgrounds){ %>
            <li><a style="text-decoration: none; color: rgb(17, 144, 194);" href="/campgrounds/<%=c._id%> ">
                    <%=c.title%>
                </a></li>
            <% } %>
    </ul>